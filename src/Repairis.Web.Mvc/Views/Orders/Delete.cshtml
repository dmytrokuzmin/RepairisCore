@model Repairis.Orders.Order

@{
    ViewBag.Title = L("Delete") + " " + L("Order{0}", Model.Id);
}
<div>
    <a asp-action="Index">@L("BackToList")</a>
</div>
<h3>@L("Order{0}", Model.Id)</h3>
<p>@L("DeleteOrderConfirmation")</p>

<form asp-action="Delete" method="post">
    <input type="submit" value="@L("Delete")" class="btn btn-default" />
</form>
